        "behat/mink-selenium2-driver": "*",
        "behat/mink-zombie-driver": "*",
        "phpunit/phpunit": "~4.7.0",
        "wdalmut/wordpress-extension": "*"
    },
    "autoload": {
        "psr-4": {"adz_stripe_donations\\": "classes/"}
    },
